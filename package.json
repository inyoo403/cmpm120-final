{
    "name": "cmpm120-final",
    "description": "A Phaser 3 template using Vite.",
    "version": "1.4.0",
    "type": "module",
    "repository": {
      "type": "git",
      "url": "git+https://github.com/inyoo403/cmpm120-final.git"
    },
    "author": "Phaser Studio <support@phaser.io> (https://phaser.io/)",
